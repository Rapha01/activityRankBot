{
  "name": "@activityrank/cli",
  "description": "CLI application for the ActivityRank Bot",
  "type": "module",
  "version": "0.0.0",
  "author": "@piemot",
  "files": [
    "dist"
  ],
  "bin": {
    "cli": "dist/cli.js",
    "__cli_bash_complete": "dist/bash-complete.js"
  },
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "prebuild": "tsc -p src/tsconfig.json",
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "postinstall": "cli install"
  },
  "tsup": {
    "entry": [
      "src/bin/cli.ts",
      "src/bin/bash-complete.ts"
    ],
    "format": [
      "esm"
    ],
    "external": [
      "discord.js"
    ],
    "tsconfig": "src/tsconfig.json",
    "clean": true,
    "splitting": true,
    "minify": true
  },
  "dependencies": {
    "@discordjs/core": "^2.0.0",
    "@discordjs/rest": "^2.4.0",
    "@inquirer/prompts": "^7.0.1",
    "@stricli/auto-complete": "^1.0.1",
    "@stricli/core": "^1.0.1",
    "chalk": "^5.3.0",
    "execa": "^9.5.0",
    "object-treeify": "^4.0.1",
    "ora": "^8.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "20.x",
    "tsup": "^6.7.0",
    "typescript": "5.6.x"
  }
}
